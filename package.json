{
  "name": "lime",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "deploy": "npm run build && npm run server",
    "server": "node ./bin/www",
    "client": "cd lemon && npm start",
    "build": "cd lemon && npm run build",
    "seed-2": "sed -i.bak s:static:build/static:g index.html",
    "seed": "sed -i.bak s:username:$USERNAME:g config/db_dump && psql -U $USERNAME lime < config/db_dump"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "body-parser": "~1.15.2",
    "concurrently": "^3.1.0",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.1",
    "debug": ">=2.6.9",
    "envify": "^4.0.0",
    "express": "~4.14.0",
    "express-session": "^1.15.1",
    "github": "^8.1.1",
    "jade": "~1.11.0",
    "moment": "^2.17.1",
    "morgan": ">=1.9.1",
    "node-gitter": "^2.0.5",
    "passport": "^0.3.2",
    "passport-github": "^1.1.0",
    "passport-gitlab2": "^2.2.0",
    "passport-oauth2": "^1.4.0",
    "pg": "^6.1.2",
    "request": "^2.79.0",
    "serve-favicon": "~2.3.0",
    "superagent": "^3.4.1",
    "supervisor": "^0.12.0"
  }
}
